<div id="mwe-pt-list">
	<div id='mwe-pt-list-control-nav-anchor'></div>
	<div id='mwe-pt-list-control-nav' class='mwe-pt-navigation-bar mwe-pt-control-gradient'>
		<div id='mwe-pt-list-control-nav-content'></div>
	</div>
	<!-- @todo Use a spinner instead of "please wait" -->
	<div id='mwe-pt-list-view'>{{ pagetriage-please-wait }}</div>
	<div id='mwe-pt-list-view-no-js'>{{ pagetriage-js-required  }}</div>
	<div id='mwe-pt-list-errors' style='display: none;'></div>
	<div id='mwe-pt-list-more' style='display: none;'>
		<a href='#' id='mwe-pt-list-more-link'>
			{{ pagetriage-more }}
		</a>
	</div>
	<div id='mwe-pt-list-load-more-anchor'></div>
	<div id='mwe-pt-list-stats-nav' class='mwe-pt-navigation-bar mwe-pt-control-gradient' style='display: none;'>
		<div id='mwe-pt-list-stats-nav-content'></div>
	</div>
	<div id='mwe-pt-list-stats-nav-anchor'></div>
</div>
